{
  "name": "generator-npm-typescript-starter",
  "version": "1.0.0",
  "description": "Generate node library boilerplate with TypeScript support.",
  "homepage": "https://github.com/umutozel/generator-npm-typescript-starter",
  "author": {
    "name": "Umut Ã–zel",
    "email": "umutozel@gmail.com",
    "url": "https://github.com/umutozel"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "node",
    "library",
    "npm",
    "typescript",
    "starter",
    "generate",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "coveralls": "^3.0.0",
    "nsp": "^2.8.0",
    "eslint": "^4.19.1",
    "prettier": "^1.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-config-xo": "^0.20.1",
    "jest": "^22.0.6"
  },
  "engines": {
    "npm": ">= 8.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "fullname": "^3.3.0",
    "git-user-name": "^2.0.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "umutozel/generator-npm-typescript-starter",
  "license": "MIT"
}
